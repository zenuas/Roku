
struct Foo<a>
	var x: a

sub f(y: Foo<String>) Foo<Int>
	print(y.x)
	var v = Foo<Int>()
	v.x = 2
	return(v)

#=>aa
#=>2
var w = Foo<String>()
w.x = "aa"
var p = f(w)
print(p.x)

