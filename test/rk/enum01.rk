
#=>zero
f(0)

#=>1
f(1)

sub f(n: Int)
	var x = f2(n)
	if i: Int = x
		print(i)
	else if s: String = x
		print(s)

sub f2(n: Int) [Int | String]
	if n == 0
		return("zero")
	else
		return(n)

###
f2の戻り値はCIL上ではObject型になる
数値型をObject型として扱うためにbox化を行う
###

###start
.assembly extern System.Console {}
.assembly extern System.Runtime {}
.assembly enum01 {}
.method public static void main()
{
    .entrypoint
    .maxstack 1
    
    ldc.i4.0
    call void f(int32)
    
    ldc.i4.1
    call void f(int32)
    ret
}
.method public static void f(int32)
{
    .maxstack 2
    .locals(
        [0] object x,
        [1] int32 i,
        [2] string s
    )
    
    ldarg.0
    call object f2(int32)
    stloc.0
    
    ldloc.0
    isinst [System.Runtime]System.Int32
    brfalse.s _ElseIf1
    ldloc.0
    unbox.any [System.Runtime]System.Int32
    stloc.1
    
    ldloc.1
    call void class [System.Console]System.Console::WriteLine(int32)
    
    br _EndIf2
    
_ElseIf1:
    
    ldloc.0
    isinst class [System.Runtime]System.String
    stloc.2
    ldloc.2
    ldnull
    cgt.un
    brfalse.s _EndIf2
    
    ldloc.2
    call void class [System.Console]System.Console::WriteLine(string)
    
_EndIf2:
    ret
}
.method public static object f2(int32)
{
    .maxstack 2
    .locals(
        [0] bool $$1
    )
    
    ldarg.0
    ldc.i4.0
    ceq
    stloc.0
    
    ldloc.0
    brfalse _Else1
    
    ldstr "zero"
    ret
    
    br _EndIf2
    
_Else1:
    
    ldarg.0
    box int32
    ret
    
_EndIf2:
    ret
}
###end
