
sub my_next(xs: Int) [Int, Int]
	return((xs, xs + 1))

sub my_isnull(xs: Int) Bool
	return(xs >= 3)

sub f(xs: Int)
	if ! my_isnull(xs)
		var x = my_next(xs)
		print(x.1)
		f(x.2)

