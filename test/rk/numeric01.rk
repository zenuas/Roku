
#=>Int
#=>1
var a = 1
print_int(a)
print(a)

#=>Long
#=>1
var b = 1
print_long(b)
print(b)

sub print_int(x: Int)
	print("Int")

sub print_long(x: Long)
	print("Long")


###
変数bがint64にマップされること
var b = 1 がint64に変換されること
print(b) がWriteLine(int64)を呼び出すこと
###

###start
.assembly extern System.Console {}
.assembly extern System.Runtime {}
.assembly numeric01 {}
.method public static void main()
{
    .entrypoint
    .maxstack 8
    .locals(
        [0] int32 a,
        [1] int64 b
    )
    
    ldc.i4.1
    stloc.0
    
    ldloc.0
    call void print_int(int32)
    
    ldloc.0
    call void [System.Console]System.Console::WriteLine(int32)
    
    ldc.i4.1
    conv.i8
    stloc.1
    
    ldloc.1
    call void print_long(int64)
    
    ldloc.1
    call void [System.Console]System.Console::WriteLine(int64)
    ret
}
.method public static void print_int(int32)
{
    .maxstack 8
    
    ldstr "Int"
    call void [System.Console]System.Console::WriteLine(string)
    ret
}
.method public static void print_long(int64)
{
    .maxstack 8
    
    ldstr "Long"
    call void [System.Console]System.Console::WriteLine(string)
    ret
}
###end
