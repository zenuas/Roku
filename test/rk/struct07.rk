
sub area(a: struct Rect(width: Int, height: Int)) Int
    return(a.width * a.height)

sub area(a: struct Triangle(width: Int, height: Int)) Int
    return((a.width * a.height) / 2)

#=>50
var s = area(Rect(10, 5))
print(s)

#=>25
var t = area(Triangle(10, 5))
print(t)

