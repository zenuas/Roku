
class Hoge<a, b>
	sub foo(a) b

sub foo(p: Int) String
	print(p)
	return("hello")

sub foo(p: String) Int
	print(p)
	return(2)

sub bar<Hoge<t1, t2>>(x: t1) t2
	var y = foo(x)
	return(y)

#=>1
#=>hello
var m = bar(1)
print(m)

#=>world
#=>2
var n = bar("world")
print(n)

###
通常、戻り値が独立したジェネリックタイプになっていると型解決できない
そこで型クラス制約で型解決を行う
###

###start
.assembly extern System.Console {}
.assembly extern System.Runtime {}
.assembly class01 {}
.method public static void main()
{
    .entrypoint
    .maxstack 1
    .locals(
        [0] string m,
        [1] int32 n
    )
    
    ldc.i4.1
    call string bar(int32)
    stloc.0
    
    ldloc.0
    call void class [System.Console]System.Console::WriteLine(string)
    
    ldstr "world"
    call int32 bar(string)
    stloc.1
    
    ldloc.1
    call void class [System.Console]System.Console::WriteLine(int32)
    ret
}
.method public static string bar(int32)
{
    .maxstack 1
    .locals(
        [0] string y
    )
    
    ldarg.0
    call string foo(int32)
    stloc.0
    
    ldloc.0
    ret
    ret
}
.method public static int32 bar(string)
{
    .maxstack 1
    .locals(
        [0] int32 y
    )
    
    ldarg.0
    call int32 'foo#1'(string)
    stloc.0
    
    ldloc.0
    ret
    ret
}
.method public static string foo(int32)
{
    .maxstack 1
    
    ldarg.0
    call void class [System.Console]System.Console::WriteLine(int32)
    
    ldstr "hello"
    ret
    ret
}
.method public static int32 'foo#1'(string)
{
    .maxstack 1
    
    ldarg.0
    call void class [System.Console]System.Console::WriteLine(string)
    
    ldc.i4.2
    ret
    ret
}
