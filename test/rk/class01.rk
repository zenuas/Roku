
class Hoge<a, b>
	sub foo(a) b

sub foo(p: Int) String
	print(p)
	return("hello")

sub foo(p: String) Int
	print(p)
	return(2)

sub bar<Hoge<t1, t2>>(x: t1) t2
	var y = foo(x)
	return(y)

#=>1
#=>hello
var m = bar(1)
print(m)

#=>world
#=>2
var n = bar("world")
print(n)

